<p id="notice"><%= notice %></p>

<h1>Unlocks</h1>

<div class="container">
    <div class="row">
      <div class="table-responsive">
        <table class="table table-hover">
  <thead>
    <tr>
      <th>IMEI</th>
      <th>Request number</th>
      <th>Date Submitted</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unlocks.each do |unlock| %>
      <tr>
        <td><%= unlock.imei %></td>
        <td><%= unlock.request_number %></td>
        <td><%= unlock.date_submitted %></td>
        <td><%= unlock.status %></td>
        <td><%= link_to 'Show', unlock %></td>
        <td><%= link_to 'Edit', edit_unlock_path(unlock) %></td>
        <td><%= link_to 'Destroy', unlock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unlock', new_unlock_path, class: "btn btn-primary"%>
