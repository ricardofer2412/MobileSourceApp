<p id="notice"><%= notice %></p>

<h1>H2O ReUps</h1>
<div class="container">
  <div class= "row">
    <div class= "table-responsive">
      <%= form_tag(h2o_re_ups_path, :method => 'get') do %>
      <%= text_field_tag :search, params[:search]%>
      <%= submit_tag "search" %>
      <% end %>
      <table class="table table-hover">

  <thead>
    <tr>

      <th><%= link_to "Sim Card", :sort => "simCard"%></th>
      <th>Phone Number</th>
      <th>Last Re-Up</th>
      <th><%= link_to "Expiration Date", :sort => "expirationDate"%></th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @h2o_re_ups.each do |h2o_re_up| %>
      <tr>

        <td><%= h2o_re_up.simCard %></td>
        <td><%= h2o_re_up.phoneNumber %></td>
        <td><%= h2o_re_up.reupDate %></td>
        <td><%= h2o_re_up.expirationDate %></td>

        <td><%= link_to 'Show', h2o_re_up %></td>
        <td><%= link_to 'Edit', edit_h2o_re_up_path(h2o_re_up) %></td>
        <td><%= link_to 'Destroy', h2o_re_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>

<br>

<%= link_to 'New H2o Re Up', new_h2o_re_up_path %>
