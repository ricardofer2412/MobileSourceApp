<%= simple_form_for(@h2o_re_up) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :companyName %>

    <%= f.input :simCard %>

    <%= f.input :phoneNumber %>

    <%= f.input :reupDate %>

    <%= f.input :expirationDate %>

    <%= f.input :pinNumber %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
