<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :customerName%>
    <%= f.input :nickname %>
    <%= f.input :simcardNumber %>
    <%= f.input :balance %>

  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <br>
<% end %>
