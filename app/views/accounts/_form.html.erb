<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
        <%= f.input :customerName%>
            <%= f.input :nickname %>

    <%= f.input :balance %>
    <%= f.input :expirationDate %>
    <%= f.input :accountStatus %>
    <%= f.input :expiredAccount %>
    <%= f.input :simcardNumber %>
    <%= f.input :phoneNumber %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
